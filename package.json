        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/pg": "^8.6.1",
        "@types/uuid": "^8.3.1",
        "@types/cors": "^2.8.12",
        "@types/node": "^16.7.10",
        "dotenv": "^10.0.0",
        "jest": "^27.0.4",
        "typescript": "^4.4.2"
